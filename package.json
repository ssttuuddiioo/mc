{
  "name": "orbit-demo",
  "version": "1.0.0",
  "description": "Production-ready map application with orbit animation and marker management",
  "main": "index.html",
  "scripts": {
    "dev": "python3 -m http.server 8000",
    "dev:cache-bust": "python3 -m http.server 8000 --bind 127.0.0.1",
    "build": "node build.js",
    "deploy": "npm run build && vercel --prod",
    "deploy:staging": "npm run build && vercel",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linter configured\" && exit 0",
    "clean": "rm -rf dist/ .vercel/",
    "version:update": "node update-version.js"
  },
  "keywords": [
    "mapbox",
    "supabase",
    "map",
    "orbit",
    "markers",
    "production"
  ],
  "author": "Studio Civic",
  "license": "MIT",
  "devDependencies": {
    "vercel": "^32.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssttuuddiioo/mc.git"
  },
  "homepage": "https://orbit-demo.vercel.app"
}
